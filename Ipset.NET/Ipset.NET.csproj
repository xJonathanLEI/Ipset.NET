<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

	<PropertyGroup>
    <IsPackable>true</IsPackable>
    <PackageVersion>0.1.1</PackageVersion>
    <PackageId>Ipset.NET</PackageId>
  	<Authors>Jonathan LEI</Authors>
		<Description>.NET wrapper library for the Linux utility program ipset</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<RepositoryUrl>https://github.com/xJonathanLEI/Ipset.NET</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageProjectUrl>https://github.com/xJonathanLEI/Ipset.NET</PackageProjectUrl>
  </PropertyGroup>

</Project>
